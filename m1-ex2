#include <iostream.h>

using namespace std;


int main()
{
    int x;
    cout<<"Please enter your grade";
    cin>>x;

    if(x>= 90 && x<=100)
    cout<<"Grade A";
      
    else if(x>=75 && x<=89)
    cout<<"Grade B";

    else if(x>=60 && x<=74)
    cout<<"Grade C";

    else if(x>=50 && x<=59)
    cout<<"Grade D";

    else(x>=0 && x<=49)
    cout<<"Grade F";

}
