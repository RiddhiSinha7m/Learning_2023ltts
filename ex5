// C++ implementation of the above approach
#include <bits/stdc++.h>
using namespace std;

void check(char ch)
{

	if (ch >= 'A' && ch <= 'Z')
		return 1;

	else if (ch >= 'a' && ch <= 'z')
		return 2;

	else if (ch >= '0' && ch<= '9')
        return 3;

	
}

// Driver Code
int main()
{
	char ch;

	// Get the character
	ch = 'A';

	// Check the character
	check(ch);

	// Get the character
	ch = 'a';

	// Check the character
	check(ch);

	// Get the character
	ch = '0';

	// Check the character
	check(ch);

	return 0;
}

